{
  "version": 2,
  "builds": [
    {
      "src": "api/leaderboard.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/leaderboard", "dest": "/api/leaderboard.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
